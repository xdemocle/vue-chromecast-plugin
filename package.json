{
  "name": "vue-chromecast-plugin",
  "version": "1.0.12",
  "description": "Vue ChromeCast plugin is a simple library using Google ChromeCast Web SDK to cast your website on supported devices and communicate with the remote web interface.",
  "main": "lib/vue-chromecast-plugin.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xdemocle/vue-chromecast-plugin.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "xdemocle <xdemocle@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xdemocle/vue-chromecast-plugin/issues"
  },
  "homepage": "https://github.com/xdemocle/vue-chromecast-plugin",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "mocha": "^4.0.1",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  }
}
